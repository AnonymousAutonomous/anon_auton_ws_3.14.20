<launch>
  <rosparam command="load" file="$(find config_manager)/configs/commands/active.yaml"/>
  <rosparam command="load" file="$(find config_manager)/configs/ports/active.yaml"/>

  <node name="cv_camera" pkg="cv_camera" type="cv_camera_node" respawn="true">
    <rosparam command="load" file="$(find config_manager)/configs/ports/active.yaml"/>
  </node>

  <node name="image_proc" pkg="image_proc" type="image_proc" ns="cv_camera" respawn="true">
  </node>

  <node name="eyes" pkg="eyes" type="new_processor" respawn="true">
  </node>

</launch>