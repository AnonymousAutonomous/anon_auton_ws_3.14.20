<launch>
  <rosparam command="load" file="$(find config_manager)/configs/commands/active.yaml"/>
  <rosparam command="load" file="$(find config_manager)/configs/ports/active.yaml"/>

  <node name="arduino" pkg="rosserial_python" type="serial_node.py" respawn="true">
    <param name="baud" value="57600"/>
    <rosparam command="load" file="$(find config_manager)/configs/ports/active.yaml"/>
  </node>
</launch>
