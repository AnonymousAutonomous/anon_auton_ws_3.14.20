<launch>
  <rosparam command="load" file="$(find config_manager)/configs/ports/active.yaml"/>

  <node name="arduino" pkg="rosserial_python" type="serial_node.py">
    <param name="baud" value="57600"/>
    <rosparam command="load" file="$(find config_manager)/configs/ports/active.yaml"/>
  </node>

  <node name="simple_motors" pkg="simple_motors" type="new_trigger">
  </node>

  <include file="$(find obstacle_detector)/launch/myNodes3_debug.launch"/>

  <rosparam command="load" file="$(find mylidar)/my_laser_config.yaml"/>
  <node name="scanfilter" pkg="laser_filters" type="scan_to_scan_filter_chain">
  </node>

  <node name="tart" pkg="obstacle_detector" type="tart_director">
  </node>

  <node name="cv_camera" pkg="cv_camera" type="cv_camera_node">
    <rosparam command="load" file="$(find config_manager)/configs/ports/active.yaml"/>
  </node>

  <node name="image_proc" pkg="image_proc" type="image_proc" ns="cv_camera">
  </node>

  <node name="image_view" pkg="image_view" type="image_view">
    <remap from="image" to="cv_camera/image_mono" />
  </node>

  <node name="eyes" pkg="eyes" type="new_processor">
  </node>

  <node name="queue" pkg="eyes" type="queue_five">
  </node>

  <node name="driver" pkg="eyes" type="baby_trilogy">
  </node>


</launch>
